{
    "rootStage": "initial",
    "repositories": {
        "phpactor": {
            "url": "https://github.com/phpactor/phpactor"
        },
        "indexer-extension": {
            "url": "https://github.com/phpactor/indexer-extension"
        },
        "class-to-file": {
            "url": "https://github.com/phpactor/class-to-file"
        }
    },
    "targets": {
        "init": {
            "tasks": [
                {
                    "type": "process",
                    "command": ["composer", "install" ]
                }
            ]
        }
    }
}
