{
    "php": {
        "phpBin": "php7.3"
    },

    "templatePaths": [
        "example/templates"
    ],
    "workspacePath": "/home/daniel/www/dantleech/maestro-two/var",

    "vars": {
        "composer.require": {
            "php": "^7.3"
        },
        "composer.require-dev": {
            "phpunit/phpunit": "^9.0"
        },
        "rector": {
            "phpunit/phpunit": [
                "config/set/phpunit70.php",
                "config/set/phpunit75.php",
                "config/set/phpunit80.php",
                "config/set/phpunit90.php"
            ]
        },
        "jobs": [
            "php-cs-fixer",
            "phpstan",
            "phpunit"
        ],
        "requireDev": [],
        "phpstanArgs": null,
        "dependencies": "highest"
    },
    "repositories": [
        {
            "name": "maestro",
            "url": "git@github.com:dantleech/maestro2",
            "vars": {
                "jobs": [
                    "psalm",
                    "phpunit"
                ]
            }
        },
        {
            "name": "worse-reflection",
            "url": "git@github.com:phpactor/worse-reflection"
        },
        {
            "name": "phpactor",
            "url": "git@github.com:phpactor/phpactor"
        },
        {
            "name": "text-document",
            "url": "git@github.com:phpactor/text-document",
            "vars": {
                "requireDev": {
                    "infection/infection": "^20.0"
                }
            }
        },
        {
            "name": "class-mover",
            "url": "git@github.com:phpactor/class-mover"
        },
        {
            "name": "class-to-file",
            "url": "git@github.com:phpactor/class-to-file"
        },
        {
            "name": "code-transform",
            "url": "git@github.com:phpactor/code-transform"
        },
        {
            "name": "completion",
            "url": "git@github.com:phpactor/completion",
            "vars": {
                "phpstanArgs": "lib"
            }
        }
    ]
