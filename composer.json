{
    "name": "dantleech/maestro2",
    "description": "Maestro the Second",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Maestro2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maestro2\\Examples\\Pipeline\\": "example/pipeline/",
            "Maestro2\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "php": "^8.0",
        "symfony/console": "^5.1",
        "dantleech/invoke": "^1.2",
        "amphp/amp": "^2.5",
        "phpactor/container": "^1.3",
        "psr/log": "1.1.x-dev",
        "amphp/process": "9999999-dev",
        "twig/twig": "9999999-dev",
        "phpactor/test-utils": "^1.0",
        "symfony/yaml": "9999999-dev",
        "symfony/filesystem": "9999999-dev",
        "composer/semver": "3.x-dev"
    },
    "require-dev": {
        "vimeo/psalm": "^4.2",
        "phpunit/phpunit": "^9.4",
        "friendsofphp/php-cs-fixer": "^2.17",
        "symfony/error-handler": "^5.2",
        "symfony/process": "^5.2"
    },
    "scripts": {
        "integrate": [
            "./vendor/bin/phpunit",
            "./vendor/bin/psalm --show-info=true",
            "PHP_CS_FIXER_IGNORE_ENV=1 ./vendor/bin/php-cs-fixer fix"
        ]
    }
}
