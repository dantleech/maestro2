{
    "name": "dantleech/maestro",
    "description": "Maestro the Second",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Maestro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maestro\\Examples\\Pipeline\\": "example/pipeline/",
            "Maestro\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "php": "^8.0",
        "symfony/console": "^5.1",
        "dantleech/invoke": "^1.2",
        "amphp/amp": "^2.5",
        "phpactor/container": "^1.3.4",
        "psr/log": "1.1.x-dev",
        "amphp/process": "^1.1",
        "symfony/process": "^5.2",
        "twig/twig": "^3.1",
        "symfony/yaml": "^5.2",
        "symfony/filesystem": "^5.2",
        "symfony/error-handler": "^5.2",
        "webmozart/path-util": "^2.3",
        "clue/arguments": "^2.1",
        "phpactor/config-loader": "^0.1.1",
        "amphp/http-client": "^4.5"
    },
    "require-dev": {
        "phpactor/test-utils": "^1.0",
        "vimeo/psalm": "^4.2",
        "phpunit/phpunit": "^9.4",
        "friendsofphp/php-cs-fixer": "^2.17"
    },
    "scripts": {
        "integrate": [
            "./vendor/bin/phpunit",
            "./vendor/bin/psalm",
            "PHP_CS_FIXER_IGNORE_ENV=1 ./vendor/bin/php-cs-fixer fix"
        ]
    },
    "bin": [
        "bin/maestro"
    ]
}
